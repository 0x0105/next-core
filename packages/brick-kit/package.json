{
  "name": "@easyops/brick-kit",
  "version": "2.2.0",
  "description": "Kernel",
  "homepage": "https://git.easyops.local/anyclouds/next-core/tree/master/packages/brick-kit",
  "license": "UNLICENSED",
  "main": "dist/index.bundle.js",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.easyops.local:anyclouds/next-core.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prestart": "npm run clean",
    "start": "concurrently -k -n tsc,build \"npm run start:types\" \"npm run start:rollup\"",
    "start:rollup": "rollup --watch --config rollup.config.js",
    "start:types": "tsc --watch --emitDeclarationOnly --declaration --declarationDir dist/types",
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:rollup",
    "build:rollup": "rollup --config rollup.config.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "docs:run": "api-extractor run",
    "docs:run:dev": "api-extractor run --local --verbose",
    "test": "cross-env NODE_ENV='test' jest",
    "test:ci": "cross-env NODE_ENV='test' CI=true jest"
  },
  "dependencies": {
    "@easyops/brick-types": "^2.1.0",
    "@sdk/auth-sdk": "^0.8.3",
    "@sdk/cmdb-sdk": "^0.15.8",
    "@sdk/micro-app-sdk": "^0.8.2",
    "@sdk/user-service-sdk": "^0.2.8",
    "minimatch": "^3.0.4"
  },
  "sideEffects": false,
  "devDependencies": {
    "@easyops/brick-http": "^2.0.0",
    "@easyops/brick-utils": "^2.0.1",
    "antd": "^4.6.3",
    "history": "^4.10.1",
    "i18next": "^19.7.0",
    "jest-websocket-mock": "^2.1.0",
    "lodash": "^4.17.20",
    "mock-socket": "^9.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.1"
  },
  "peerDependencies": {
    "@easyops/brick-http": "*",
    "@easyops/brick-utils": "*",
    "antd": "*",
    "history": "*",
    "i18next": "*",
    "lodash": "*",
    "react": "*",
    "react-dom": "*",
    "react-i18next": "*"
  }
}
