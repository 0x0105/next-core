{
  "name": "@easyops/brick-icons",
  "version": "0.4.1",
  "description": "Self-designed icons",
  "homepage": "https://git.easyops.local/anyclouds/next-core/tree/master/packages/brick-icons",
  "license": "UNLICENSED",
  "main": "dist/index.bundle.js",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.easyops.local:anyclouds/next-core.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prestart": "npm run clean && node scripts/pre-build.js",
    "start": "npm run build:types && rollup --config rollup.config.js --watch",
    "prebuild": "npm run clean && node scripts/pre-build.js",
    "build": "npm run build:types && npm run build:rollup",
    "build:rollup": "rollup --config rollup.config.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "test": "cross-env NODE_ENV='test' jest",
    "test:ci": "cross-env NODE_ENV='test' CI=true jest"
  },
  "sideEffects": false,
  "devDependencies": {
    "@svgr/rollup": "^4.3.3",
    "change-case": "^3.1.0",
    "fs-extra": "^8.1.0",
    "klaw-sync": "^6.0.0",
    "prettier": "^1.18.2",
    "react": "^16.10.2"
  },
  "peerDependencies": {
    "react": "*"
  }
}
