{
  "name": "next-core",
  "private": true,
  "homepage": "https://git.easyops.local/anyclouds/next-core",
  "repository": {
    "type": "git",
    "url": "git@git.easyops.local:anyclouds/next-core.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "lerna bootstrap",
    "start": "lerna run start --scope \"@easyops/brick-container\"",
    "remote": "lerna run start --scope \"@easyops/brick-container\" -- --remote",
    "build": "lerna run build",
    "postbuild": "npm run size-limit",
    "size-limit": "size-limit",
    "test": "cross-env NODE_ENV='test' jest",
    "test:ci": "cross-env NODE_ENV='test' CI=true jest --ci",
    "yo": "brick-scripts -t dll",
    "yo-debug": "node --inspect-brk ./packages/brick-scripts/dist/index.js -t dll"
  },
  "browserslist": {
    "development": [
      "extends @easyops/browserslist-config-next/development"
    ],
    "production": [
      "extends @easyops/browserslist-config-next/production"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/types": "^7.9.0",
    "@size-limit/file": "^4.4.5",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/fs-extra": "^8.1.0",
    "@types/history": "^4.7.5",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/js-yaml": "^3.12.3",
    "@types/klaw-sync": "^6.0.0",
    "@types/lodash": "^4.14.149",
    "@types/meow": "^5.0.0",
    "@types/node": "^10.12.18",
    "@types/prettier": "^1.19.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "babel-jest": "^25.2.6",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "lerna": "^3.20.1",
    "lint-staged": "^10.1.2",
    "null-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "typescript": "~3.8.3"
  },
  "workspaces": [
    "packages/*",
    "dll/*"
  ],
  "resolutions": {
    "@types/react": "^16.9.32",
    "lodash": "^4.17.15"
  }
}
