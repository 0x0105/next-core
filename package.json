{
  "name": "next-core",
  "private": true,
  "homepage": "https://git.easyops.local/anyclouds/next-core",
  "repository": {
    "type": "git",
    "url": "git@git.easyops.local:anyclouds/next-core.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "lerna bootstrap",
    "start": "lerna run start --scope \"@easyops/brick-container\"",
    "remote": "lerna run start --scope \"@easyops/brick-container\" -- --remote",
    "build": "lerna run build",
    "test": "cross-env NODE_ENV='test' jest",
    "test:ci": "cross-env NODE_ENV='test' CI=true jest --ci",
    "yo": "brick-scripts -t dll",
    "yo-debug": "node --inspect-brk ./packages/brick-scripts/dist/index.js -t dll"
  },
  "browserslist": {
    "development": [
      "extends @easyops/browserslist-config-next/development"
    ],
    "production": [
      "extends @easyops/browserslist-config-next/production"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/fs-extra": "^8.0.1",
    "@types/history": "^4.7.3",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.24",
    "@types/js-yaml": "^3.12.1",
    "@types/klaw-sync": "^6.0.0",
    "@types/lodash": "^4.14.149",
    "@types/meow": "^5.0.0",
    "@types/node": "^10.12.18",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "lerna": "^3.19.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "typescript": "~3.7.3"
  },
  "workspaces": [
    "packages/*",
    "dll/*"
  ],
  "resolutions": {
    "@types/react": "^16.9.17",
    "lodash": "^4.17.15"
  }
}
